module Base.Vector

import Base.Nat

Vector : Nat -> Type -> Type
Vector = \ n a
  .  { x : Nat -> Type }
  -> x z
  -> ({ n : Nat } -> a -> x n -> x (s n))
  -> x n

nil : Vector z a
nil = \ nil _ . nil

cons : { n : Nat } -> a -> Vector n a -> Vector (s n) a
cons = \ h t nil cons . cons h (t nil cons)
